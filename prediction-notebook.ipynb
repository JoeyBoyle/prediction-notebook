    "## 3. Make Predictions"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8ebe1c1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# -------------------------- USER INPUT --------------------------\n",
    "# Customer Data for Prediction\n",
    "new_customer_data = {\n",
    "    \"gender\": \"Male\",\n",
    "    \"SeniorCitizen\": \"No\",\n",
    "    \"Partner\": \"No\",\n",
    "    \"Dependents\": \"No\",\n",
    "    \"tenure\": 33,\n",
    "    \"PhoneService\": \"Yes\",\n",
    "    \"MultipleLines\": \"No\",\n",
    "    \"InternetService\": \"Fiber optic\",\n",
    "    \"OnlineSecurity\": \"No\",\n",
    "    \"OnlineBackup\": \"No\",\n",
    "    \"DeviceProtection\": \"No\",\n",
    "    \"TechSupport\": \"No\",\n",
    "    \"StreamingTV\": \"No\",\n",
    "    \"StreamingMovies\": \"No\",\n",
    "    \"Contract\": \"Month-to-month\",\n",
    "    \"PaperlessBilling\": \"No\",\n",
    "    \"PaymentMethod\": \"Electronic check\",\n",
    "    \"MonthlyCharges\": 68.25,\n",
    "    \"TotalCharges\": 2171.15,\n",
    "}\n",
    "\n",
    "# Select a model for prediction (CatBoostModel, RandomForestModel, XGBoostModel, LogisticRegressionModel)\n",
    "selected_model = CatBoostModel\n",
    "# ----------------------------------------------------------------\n",
    "\n",
    "# Convert to DataFrame\n",
    "input_df = pd.DataFrame([new_customer_data])\n",
    "\n",
    "# Clean Data for Prediction\n",
    "binary_features = [\n",
    "    'Partner', 'Dependents', 'PhoneService', \n",
    "    'OnlineSecurity', 'OnlineBackup', \n",
    "    'DeviceProtection', 'TechSupport', 'StreamingTV', \n",
    "    'StreamingMovies', 'PaperlessBilling'\n",
    "]\n",
    "\n",
    "for feature in binary_features:\n",
    "    input_df[feature] = input_df[feature].map({'Yes': 1, 'No': 0})\n",
    "\n",
    "input_df['SeniorCitizen'] = input_df['SeniorCitizen'].map({'Yes': 1, 'No': 0})\n",
    "input_df['MultipleLines'] = input_df['MultipleLines'].map({'Yes': 1, 'No': 0, 'No phone service': 0})\n",
    "input_df['gender'] = input_df['gender'].map({'Male': 1, 'Female': 0})\n",
    "\n",
    "contract_mapping = {\n",
    "    'Month-to-month': 0,\n",
    "    'One year': 1,\n",
    "    'Two year': 2\n",
    "}\n",
    "input_df['Contract'] = input_df['Contract'].map(contract_mapping)\n",
    "\n",
    "internet_mapping = {\n",
    "    'DSL': 0,\n",
    "    'Fiber optic': 1,\n",
    "    'No': 2\n",
    "}\n",
    "input_df['InternetService'] = input_df['InternetService'].map(internet_mapping)\n",
    "\n",
    "payment_mapping = {\n",
    "    'Electronic check': 0,\n",
    "    'Mailed check': 1,\n",
    "    'Bank transfer (automatic)': 2,\n",
    "    'Credit card (automatic)': 3\n",
    "}\n",
    "input_df['PaymentMethod'] = input_df['PaymentMethod'].map(payment_mapping)\n",
    "\n",
    "# Make Prediction\n",
    "prediction = selected_model.predict(input_df)[0]\n",
    "probability = selected_model.predict_proba(input_df)[0]\n",
    "print(\"Prediction result for the new data row:\", \"Does Churn\" if prediction == 1 else \"Does not Churn\")\n",
    "print(\"Confidence:\", round(probability[prediction]*100, 2), \"%\")"
   ]
